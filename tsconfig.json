{
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": [
      "ESNext"
    ],
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "ES2018"
  },
  "exclude": [
    "dist",
    "eslint.config.mjs",
    "node_modules"
  ]
}
